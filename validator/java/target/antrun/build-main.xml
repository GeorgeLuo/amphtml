<?xml version="1.0" encoding="UTF-8" ?>
<project name="maven-antrun-" default="main"  >
<target name="main">
  <property name="protoc.filename" value="protoc-3.8.0-${os.detected.classifier}.exe"/>
  <property name="protoc.filepath" value="/Users/gluo17/Documents/workspace/amphtml/validator/java/target/${protoc.filename}"/>
  <chmod file="${protoc.filepath}" perm="ugo+rx"/>
  <mkdir dir="/Users/gluo17/Documents/workspace/amphtml/validator/java/src/main/java"/>
